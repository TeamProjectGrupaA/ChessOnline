package rests.Models;

import java.io.Serializable;

import javax.persistence.*;


@Entity
@Table(name = "games")
public class Games implements Serializable {
 
    private static final long serialVersionUID = -63347845263705435L;
    private int id;
    private String fen;

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    @Column(name = "id", nullable = false, insertable = true, updatable = true)
    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    @Basic
    @Column(name = "fen", nullable = true, insertable = true, updatable = true, length = 255)
    public String getFen() {
        return fen;
    }

    public void setFen(String fen) {
        this.fen = fen;
    }

    @Override
    public boolean equals(Object o) {
        if (this == o) return true;
        if (o == null || getClass() != o.getClass()) return false;

        Games games = (Games) o;

        if (id != games.id) return false;
        if (fen != null ? !fen.equals(games.fen) : games.fen != null) return false;

        return true;
    }

    @Override
    public int hashCode() {
        int result = id;
        result = 31 * result + (fen != null ? fen.hashCode() : 0);
        return result;
    }
}